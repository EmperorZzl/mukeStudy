{
  "name": "koa-hotdev-es6-webpack",
  "version": "1.0.0",
  "description": "## 中间件nodemon 检测js的变化",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --exec babel-node src/index.js",
    "webpack:debug": "npx node --inspect-brk  ./node_modules/.bin/webpack --inline --progress",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.pro.js",
    "dev": "cross-env NODE_EN=dev nodemon --exec babel-node --inspect ./src/index.js",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-combine-routers": "^4.0.2",
    "koa-helmet": "^6.0.0",
    "koa-json": "^2.0.2",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "webpack": "^5.5.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
